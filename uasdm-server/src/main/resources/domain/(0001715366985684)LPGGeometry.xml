<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdBusiness name="gov.geoplatform.uasdm.LPGGeometry" table="lpg_geometry" label="LPG Geometry" >
        <attributes>
          <reference name="version" label="Version" type="net.geoprism.graph.LabeledPropertyGraphTypeVersion" column="version"  required="true"/>
          <text name="typeCode" label="typeCode" required="true" column="type_code"/>
          <uuid name="locationOid" label="Location Oid" required="true" column="location_oid"/>
          <text name="locationCode" label="Location Code" required="true" column="location_code"/>
          <text name="locationLabel" label="Location Label" required="true" column="location_label"/>
          <text name="locationUuid" label="Location UUID" required="true" column="location_uuid"/>
          <text name="parent" label="Parent" required="false"/>
          <shape name="geometry" label="Geometry" required="false" srid="4326" dimension="2" />
        </attributes>
      </mdBusiness>

      <mdIndex type="gov.geoplatform.uasdm.LPGGeometry" label="Geometry Version Type" active="true" unique="false">
        <attribute name="version" indexOrder="0"/>
