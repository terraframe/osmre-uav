        <attribute name="version" indexOrder="0"/>
        <attribute name="typeCode" indexOrder="1"/>
        <attribute name="locationOid" indexOrder="2"/>
      </mdIndex>

    </create>
    <permissions>
      <role roleName="PUBLIC">
        <grant>
          <mdBusinessPermission type="gov.geoplatform.uasdm.LPGGeometry">
            <operation name="ALL"/>
          </mdBusinessPermission>
        </grant>
      </role>
    </permissions>
  </doIt>
  <undoIt>
    <delete>
      <object type="com.runwaysdk.system.metadata.MdBusiness" key="gov.geoplatform.uasdm.LPGGeometry" />
    </delete>
  </undoIt>
