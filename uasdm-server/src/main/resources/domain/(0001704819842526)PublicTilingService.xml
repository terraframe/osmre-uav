<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdUtil
         name="gov.geoplatform.uasdm.tile.TileServiceAuthenticator"
         label="Tile Service Authenticator">
        <mdMethod
           name="authenticate"
           label="authenticate"
           static="false"
           returnType="void">
        </mdMethod>
      </mdUtil>
    </create>
    <update></update>
    <permissions>
      <role
         roleName="PUBLIC">
        <grant>
          <mdBusinessPermission
             type="gov.geoplatform.uasdm.tile.TileServiceAuthenticator">
            <mdMethodPermission
               methodName="authenticate">
              <operation name="EXECUTE" />
            </mdMethodPermission>
          </mdBusinessPermission>
        </grant>
      </role>
      <method
         methodName="authenticate"
         type="gov.geoplatform.uasdm.tile.TileServiceAuthenticator">
        <grant>
          <mdBusinessPermission
             type="gov.geoplatform.uasdm.graph.Product">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
          <mdBusinessPermission
             type="gov.geoplatform.uasdm.graph.Document">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
        </grant>
      </method>
    </permissions>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>