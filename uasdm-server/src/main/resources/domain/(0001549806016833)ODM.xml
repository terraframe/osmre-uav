<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException name="gov.geoplatform.uasdm.odm.UnreachableHostException" message="Could not connect the odm server. Please contact your server administrator.">
      </mdException>
      
      <mdBusiness name="gov.geoplatform.uasdm.odm.ODMProcessingTask" label="ODM Processing" extends="gov.geoplatform.uasdm.bus.WorkflowTask">
        <attributes>
         <char name="odmUUID" size="64"/>
        </attributes>
      </mdBusiness>
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete>
      <object key="gov.geoplatform.uasdm.odm.HttpConnectionException" type="com.runwaysdk.system.metadata.MdException"/>
      <object key="gov.geoplatform.uasdm.odm.ODMProcessingTask" type="com.runwaysdk.system.metadata.MdBusiness"/>
    </delete>
  </undoIt>
</version>
